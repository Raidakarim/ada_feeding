<root main_tree_to_execute = "Root" >
  <BehaviorTree ID="Root">
    <ReactiveSequence>
      <CheckWatchdog />
      <SubTree ID="Meal"/>
    </ReactiveSequence>
  </BehaviorTree>

  <BehaviorTree ID="Meal">
    <Sequence>
      <PerceiveFood timeout="5" name_filter="cantaloupe" objects="{foods}" />
      <PerceiveFace timeout="1" objects="{face}" />
    </Sequence>
  </BehaviorTree>  
</root>